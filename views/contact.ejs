<script type="text/javascript">


	$('#contactUs').submit(function (e) {
		e.preventDefault();
		//{name:req.body.firstname,s_name:req.body.lastname,subject:req.body.subject}
		var full_name = $('#full_name').val();
		var phone = $('#phone').val();
		var website = $('#website').val();
		var email = $('#cuemail').val();
		var message = $('#message').val();

		if (full_name == "" || phone == "" || website == "" || email == "" || message == "") {

			alert ("parameters missing");
			
		} else {

			$.post('/contact', { name: full_name, phone: phone, email: email, website: website, message: message,}, function (res) {
				if (res.code == 0) {
					// new PNotify({
					//    title: 'Error',
					//    text: res.message,
					//    type: 'error'
					// });
					alert ("oops");

				} else if (res.message == "success") {
					alert("done");
				}
			});
		}
	});
	
</script>







